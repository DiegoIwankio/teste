<!doctype html>
<html>

<head>
    {{> meta}}

{{#if stylesheets}}
    {{#each stylesheets}}
        <link rel="stylesheet" href="{{assets}}/assets/{{.}}">
    {{/each}}
{{/if}}
{{#if cdncss}}
    {{#each cdncss}}
        <link rel="stylesheet" href="{{.}}">
    {{/each}}
{{/if}}

    {{!-- {{> analytic}}
    --}}
  </head>

        <body class="{{#if boXed}}boxed{{/if}} {{#if sideMini}}sidebar-left-mini{{/if}} {{#if fixedMenu}}menu-affix{{/if}}">
            <div class="bg-dark dk" id="wrap">
                <div id="top">
                    {{> nav }}
                    {{#if noHeader}}
                    {{else}}
                        <header class="head">
                            {{#if noMainSearch}}
                            {{else}}
                                <div class="search-bar">
                                    {{> mainSearch }}
                                </div>
                                <!-- /.search-bar -->
                            {{/if}}
                            <div class="main-bar">
                                <h3>
            {{#if icon}}
              <i class="{{ icon }}"></i>&nbsp;
            {{/if}}
            {{ title }}
          </h3>
                            </div>
                            <!-- /.main-bar -->
                        </header>
                        <!-- /.head -->
                    {{/if}}
                </div>
                <!-- /#top -->
                {{#if noLeftSidebar}}
                {{else}}
                    {{> left }}
                {{/if}}
                <div id="content">
                    <div class="outer">
                        <div class="inner bg-light lter">
                            {% body %}
                        </div>
                        <!-- /.inner -->
                    </div>
                    <!-- /.outer -->
                </div>
                <!-- /#content -->

                {{#if noRightSidebar}}
                {{else}}
                    {{> right }}
                {{/if}}
            </div>
            <!-- /#wrap -->
            <footer class="Footer bg-dark dker">
                <p>2017 &copy; Metis Bootstrap Admin Template v2.4.2</p>
            </footer>
            <!-- /#footer -->
            {{> helpModal }}

            <!--jQuery -->
            <script src="{{assets}}/lib/jquery/jquery.min.js"></script>

            {{#if cdnjs}}
                {{#each cdnjs}}
                    <script src="{{.}}"></script>
                {{/each}}
            {{/if}}

            <!--Bootstrap -->
            <script src="{{assets}}/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
            <!-- MetisMenu -->
            <script src="{{assets}}/lib/metismenu/metisMenu.min.js"></script>
            <!-- Screenfull -->
            <script src="{{assets}}/lib/screenfull/screenfull.js"></script>

            {{#if javascripts}}
                {{#each javascripts}}
                    <script src="{{assets}}/assets/{{.}}"></script>
                {{/each}}
            {{/if}}

            <!-- Metis core scripts -->
            <script src="{{assets}}/js/core.js"></script>
            <!-- Metis demo scripts -->
            <script src="{{assets}}/js/app.js"></script>

            {{#if scripts}}
                <script>
                    $(function() {
                    {{#each scripts }}
                      {{.}};
                    {{/each}}
                    });
                </script>
            {{/if}}

        </body>

</html>
